{
	"_version": "1.5.0",
	"sap.app": {
		"id": "customer.customer",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "hanatemplates.basicSAPUI5ApplicationProject",
			"version": "0.0.0"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "customer.customer.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "customer.customer.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "customer.customer.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl"
			},
			"routes": [{
				"name": "Menu",
				"pattern": "/Restaurant/{RestaurantId}/Menu",
				"target": "menu"
			}, {
				"name": "Login",
				"pattern": "/Login:?query:",
				"target": "login"
			}, {
				"name": "Cart",
				"pattern": "/Restaurant/{RestaurantId}/Cart/{OrderPath}",
				"target": "cart"
			}, {
				"name": "OrderCreated",
				"pattern": "/Restaurant/{RestaurantId}/OrderCreated",
				"target": "orderCreated"
			}],
			"targets": {
				"menu": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "Menu.Menu"
				},
				"login": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "Login.Login"
				},
				"cart": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "Cart.Cart"
				},
				"orderCreated": {
					"viewType": "XML",
					"transition": "flip",
					"viewName": "Checkout.OrderCreated"
				}
			}
		}
	}
}