{
	"_version": "1.5.0",

	"sap.app": {
		"id": "restaurants.ui5",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "hanatemplates.basicSAPUI5ApplicationProject",
			"version": "0.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"handleValidation": true,
		"rootView": {
			"viewName": "restaurants.ui5.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "restaurants.ui5.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "restaurants.ui5.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl"
			},
			"routes": [{
				"name": "AdminHome",
				"pattern": "",
				"target": "adminHome"
			}, {
				"name": "ManagerHome",
				"pattern": "ManagerHome/{RestaurantId}",
				"target": "managerHome"
			}, {
				"name": "Menu",
				"pattern": "Menu/{RestaurantId}",
				"target": "menu"
			}, {
				"name": "Staff",
				"pattern": "Staff",
				"target": "staff"
			}, {
				"name": "NewOrder",
				"pattern": "Restaurant/{RestaurantId}/NewOrder",
				"target": "newOrder"
			}],
			"targets": {
				"adminHome": {
					"viewType": "XML",
					"viewName": "AdminHome",
					"viewLevel": 0
				},
				"managerHome": {
					"viewType": "XML",
					"viewName": "ManagerHome",
					"viewLevel": 0
				},
				"menu": {
					"viewType": "XML",
					"viewName": "Menu.Menu",
					"viewLevel": 0
				},
				"staff": {
					"viewType": "XML",
					"viewName": "Staff.Staff",
					"viewLevel": 1
				},
				"newOrder": {
					"viewType": "XML",
					"viewName": "Order.NewOrder",
					"viewLevel": 1
				}
			}
		}
	}
}