PROCEDURE "restaurants.db.procedures::CreateUser"( 
	 IN I_USERNAME VARCHAR(30),
	 IN I_PASSWORD VARCHAR(30)
)
   LANGUAGE SQLSCRIPT
   SQL SECURITY DEFINER
   --DEFAULT SCHEMA <default_schema_name>
   AS
  
BEGIN

	EXEC ( 'CREATE USER ' || I_USERNAME || ' PASSWORD ' || I_PASSWORD || ' SET PARAMETER CLIENT = ''001''');
	
END