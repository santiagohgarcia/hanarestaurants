VIEW "restaurants.db.views::TodayOrders" 
AS SELECT RO.* FROM "restaurants.db::RestaurantsContext.RestaurantOrder" as RO
	inner join "restaurants.db::RestaurantsContext.Restaurant" AS "R"
	on "R"."RestaurantId" = RO."RestaurantId"
	WHERE RO."ModifiedAt" > "restaurants.db.functions::GetLastDateTimeClose"("R"."CloseDayHour").PREVIOUSDATETIMECLOSE